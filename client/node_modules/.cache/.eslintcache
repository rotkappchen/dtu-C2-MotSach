nst editGenre = async (id, name) =>{\r\n        setID(id)\r\n        setGenre(name)\r\n        setOnEdit(true)\r\n    }\r\n\r\n    const deleteGenre = async id =>{\r\n        try {\r\n            const res = await axios.delete(`/api/genre/${id}`, {\r\n                headers: {Authorization: token}\r\n            })\r\n            alert(res.data.msg)\r\n            setCallback(!callback)\r\n        } catch (err) {\r\n            alert(err.response.data.msg)\r\n        }\r\n    }\r\n\r\n    return (\r\n        <div className=\"categories\">\r\n            <form onSubmit={createGenre}>\r\n                <label htmlFor=\"category\">Genre</label>\r\n                <input type=\"text\" name=\"category\" value={genre} required\r\n                onChange={e => setGenre(e.target.value)} />\r\n\r\n                <button type=\"submit\">{onEdit? \"Update\" : \"Create\"}</button>\r\n            </form>\r\n\r\n            <div className=\"col\">\r\n                {\r\n                    genres.map(genre => (\r\n                        <div className=\"row\" key={genre._id}>\r\n                            <p>{genre.title}</p>\r\n                            <div>\r\n                                <button onClick={() => editGenre(genre._id, genre.name)}>Edit</button>\r\n                                <button onClick={() => deleteGenre(genre._id)}>Delete</button>\r\n                            </div>\r\n                        </div>\r\n                    ))\r\n                }\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Categories\r\n","D:\\University\\CAPSTONE 2\\MotSach\\client\\src\\components\\body\\addbook\\AddBook.js",["373","374","375"],[],"D:\\University\\CAPSTONE 2\\MotSach\\client\\src\\components\\utils\\rating\\Rating.js",[],[],"D:\\University\\CAPSTONE 2\\MotSach\\client\\src\\components\\utils\\formInput\\FormInput.js",[],[],"D:\\University\\CAPSTONE 2\\MotSach\\client\\src\\components\\utils\\FetchData.js",[],[],"D:\\Univers